appl: seqret [
  documentation: "Reads and writes (returns) sequences"
  groups: "Edit"
]

bool: feature [
  information: "Use feature information"
]

seqall: sequence  [
  parameter: "Y"
  feature: "$(feature)"
]

seqoutall: outseq  [
  parameter: "Y"
  feature: "$(feature)"
]

bool: firstonly  [
  information: "Read one sequence and stop"
]
