application: whichdb [
  documentation: "Search all databases for an entry"
  groups: "Information"
  gui: "yes"
  batch: "yes"
  cpu: "low"
]

string: entry  [
  parameter: "Y"
  information: "ID or Accession number"
]

boolean: get  [
  required: "N"
  default: "N"
  information: "Retrieve sequences"
]

boolean: showall  [
  required: "N"
  default: "N"
  information: "Show failed attempts"
]

outfile: outfile  [
  required: "@(!$(get))"
  nullok: "$(get)"
]
