application: silent [
  documentation: "Silent mutation restriction enzyme scan"
  groups: "Nucleic:Restriction"
]

section: input [
  information: "input Section"
  type: "page"
]

  sequence: seq  [
    parameter: "Y"
  ]

endsection: input

section: required [
  information: "required Section"
  type: "page"
]

  string: enzymes  [
    required: "Y"
    default: "all"
    information: "Comma separated enzyme list"
  ]

endsection: required

section: output [
  information: "output Section"
  type: "page"
]

  boolean: sshow  [
    required: "N"
    default: "N"
    information: "Display untranslated sequence"
  ]

  boolean: tshow  [
    required: "N"
    default: "N"
    information: "Display translated sequence"
  ]

  boolean: allmut  [
    required: "N"
    default: "N"
    information: "Display all mutations"
  ]

  outfile: outf  [
    parameter: "Y"
  ]

endsection: output
