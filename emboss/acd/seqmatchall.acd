application: seqmatchall [
  documentation: "Does an all-against-all comparison of a set
                  of sequences"
  groups: "Alignment:Local"
]

section: input [
  information: "input Section"
  type: "page"
]

  seqset: sequence  [
    parameter: "Y"
    type: "any"
  ]

endsection: input

section: required [
  information: "required Section"
  type: "page"
]

  integer: wordsize  [
    required: "Y"
    default: "4"
    minimum: "2"
    information: "Word size"
  ]

endsection: required

section: output [
  information: "output Section"
  type: "page"
]

  outfile: outfile  [
    parameter: "Y"
  ]

endsection: output
