application: entrailshtml [
  documentation: "Reports the internal structures in HTML"
  groups: "Utils:Misc"
    relations: "0000220 topic File handling"
    relations: "0000335 operation File reformatting"
]

section: required [
  information: "required section"
  type: "page"
]

  list: tables [
    standard: "Y"
    default: "all"
    minimum: "1"
    value: "all: All tables;
           inseq: Input sequence formats;
           outseq: Output sequence formats"
    information: "Tables to be written"
    relations: "0002152 data Rendering parameter"
  ]

endsection: required

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "html"
    relations: "0000969 data Free text"
  ]

endsection: output
