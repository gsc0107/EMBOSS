application: profit [
  documentation: "Scan one or more sequences with a simple frequency matrix"
  groups: "Nucleic:Profiles, Protein:Profiles"
    relations: "0000188 topic Sequence profiles"
    relations: "0000300 operation Sequence-profile alignment"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: infile [
    parameter: "Y"
    information: "Profile or weight matrix file"
    knowntype: "profile or matrix"
    relations: "0001354 data Sequence profile"
  ]

  seqall: sequence [
    parameter: "Y"
    type: "any"
    relations: "0000848 data Sequence"
    relations: "0000849 data Sequence record"
    relations: "0002178 data 1 or more"
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "profit output"
    relations: "0000869 data Sequence-profile alignment"
  ]

endsection: output
