application: ehmmindex [
  documentation: "Create a binary SSI index for an HMM
                  database."
  groups: "HMM"
  gui: "yes"
  batch: "yes"
  cpu: "medium"
  embassy: "hmmernew"
]

section: input [
  information: "Input section"
  type: "page"
]

  infile: database [
    parameter: "Y"
    information: "HMMER hmm database file"
    knowntype: "hmm database"
    help: "HMM database."
  ]

endsection: input
