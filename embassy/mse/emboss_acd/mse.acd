application: mse [
  documentation: "Multiple Sequence Editor"
  groups: "Alignment:multiple"
  gui: "N"
  embassy: "mse"
]

section: input [
  information: "Input section"
  type: "page"
]

  seqset: sequence [
    parameter: "Y"
    help: "File containing a sequence alignment"
    type: "gapany"
    aligned: "N"
    nullok: "Y"
  ]

endsection: input

section: additional [
  information: "Additional section"
  type: "page"
]

  boolean: gcgmode [
    information: "Use GCG Unix mode for keyboard shortcuts and commands"
    default: "N"
  ]
  
  boolean: protein [
    information: "Sequences are protein"
    default: "$(acdprotein)"
  ]
  
endsection: additional

section: advanced [
  information: "Advanced section"
  type: "page"
]

endsection: advanced

section: output [
  information: "Output section"
  type: "page"
]

  seqoutset: outseq [
    parameter: "Y"
  ]

endsection: output
