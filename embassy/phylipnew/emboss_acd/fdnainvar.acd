application: fdnainvar [
  documentation: "Nucleic acid sequence Invariants method"
  groups: "Phylogeny:Molecular sequence"
]

section: input [
  information: "Input section"
  type: "page"
]

seqsetall: sequence  [
  parameter: "Y"
  type: "gapdna"
]

properties: weights [
  standard: "Y"
  help: "Weights file"
  nullok: "Y"
  knowntype: "weights"
]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

outfile: outfile  [
  parameter: "Y"
  knowntype: "dnainvar output"
]

boolean: printdata  [
  additional: "Y"
  default: "N"
  information: "Print out the data at start of run"
]

boolean: printdotdiff [
  additional: "Y"
  default: "Y"
  information: "Use dot-differencing to display data"
]

boolean: printpattern [
  additional: "Y"
  default: "Y"
  information: "Print out the counts of patterns"
]

boolean: printinvariant  [
  additional: "Y"
  default: "Y"
  information: "Print out the invariants"
]

boolean: progress  [
  additional: "Y"
  default: "N"
  information: "Print indications of progress of run"
]

endsection: output
