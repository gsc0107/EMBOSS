application: fdnamlk [
  documentation: "Estimates phylogenies from nucleic acid
                  sequence Maximum Likelihood with molecular clock"
  groups: "Phylogeny:Molecular sequence"
  gui: "yes"
  batch: "yes"
  cpu: "high"
]

seqset: sequences  [
  parameter: "Y"
  type: "gapany"
  help: "File containing a sequence allignment"
]

outfile: outfile  [
  parameter: "Y"
  knowntype: "dnamlk output"
]

boolean: besttree  [
  additional: "Y"
  default: "Y"
  information: "Search for best tree"
]

boolean: lengths  [
  standard: "@(!$(besttree))"
  default: "N"
  information: "Use lengths from user trees"
]

boolean: global  [
  standard: "$(besttree)"
  default: "N"
  information: "Global rearrangements"
]

boolean: random  [
  standard: "$(besttree)"
  default: "N"
  information: "Randomize input order of species"
]

integer: randseed  [
  standard: "$(random)"
  default: "3"
  information: "Random number seed (must be odd)"
]

integer: randtimes  [
  standard: "$(random)"
  default: "3"
  information: "How many times to randomise"
]

float: ttratio  [
  additional: "Y"
  default: "2.0"
  information: "Transition/transversion ratio"
]

bool: basefrequency [
  additional: "Y"
  default: "Y"
  information: "Use empirical base frequencies"
]

float: freqa  [
  standard: "@(!$(basefrequency))"
  minimum: "0.0"
  maximum: "1.0"
  default: "0.25"
  information: "Frequency for A"
]

float: freqc  [
  standard: "@(!$(basefrequency))"
  minimum: "0.0"
  maximum: "1.0"
  default: "0.25"
  information: "Frequency for C"
]

float: freqg  [
  standard: "@(!$(basefrequency))"
  minimum: "0.0"
  maximum: "1.0"
  default: "0.25"
  information: "Frequency for G"
]

float: freqt  [
  standard: "@(!$(basefrequency))"
  minimum: "0.0"
  maximum: "1.0"
  default: "0.25"
  information: "Frequency for T/U"
]

boolean: categories  [
  additional: "Y"
  default: "N"
  information: "More than one category of substitution rates"
]

integer: catnum  [
  standard: "$(categories)"
  minimum: "2"
  maximum: "9"
  default: "2"
  information: "Number of categories of substitution rates"
]

string: catvals  [
  standard: "$(categories)"
  information: "space seperated category values"
]

string: catprob [
  standard: "$(categories)"
  information: "space seperated probabillity values"
]

boolean: autog [
  standard: "$(categories)"
  default: "N"
  information: "Use Defualt Mean block length of sites"
]

float: lambda  [
  standard: "@(!$(autog))"
  minimum: "1.0"
  default: "1.0"
  information: "Mean block length of sites having the same rate
                (greater than 1)"
]

boolean: printdata  [
  additional: "Y"
  default: "N"
  information: "Print out the data at start of run"
]

boolean: progress  [
  additional: "Y"
  default: "N"
  information: "Print indications of progress of run"
]

boolean: drawtree  [
  additional: "Y"
  default: "Y"
  information: "Draw tree"
]

boolean: trout  [
  parameter: "Y"
  default: "Y"
  information: "Create a tree file"
]

outfile: treefile  [
  standard: "$(trout)"
  extension: "treefile"
  information: "Tree file name"
  knowntype: "Newick tree"
]
