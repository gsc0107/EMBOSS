application: ohmmfetch [
  documentation: "Extract HMM from a database"
  groups: "HMM"
  gui: "yes"
  batch: "yes"
  cpu: "medium"
  embassy: "hmmer"
]

section: input [
  information: "Input section"
  type: "page"
]

  string: database [
    parameter: "Y"
    information: "Database name"
    knowntype: "hmm database name"
  ]

  string: name [
    parameter: "Y"
    information: "Entry name"
    knowntype: "hmm entry name"
  ]

endsection: input

section: output [
  information: "Output section"
  type: "page"
]

  outfile: outfile [
    parameter: "Y"
    knowntype: "hmm"
    information: "HMMER hidden markov model output file"
  ]

endsection: output
